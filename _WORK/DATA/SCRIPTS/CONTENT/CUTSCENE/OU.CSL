		[]
	[]
	[% zCCSBlock 0 17782]
		blockName=string:DIA_BaalTaran_Bruderschaft_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17783]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17784]
				subType=enum:0
				text=string:Unser Lager liegt östlich von hier, im Sumpfgebiet. Ich bin hergekommen, um Fremden wie dir die Lehre des Schläfers zu verkünden. 
				name=string:DIA_BaalTaran_Bruderschaft_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17785]
		blockName=string:DIA_BaalTaran_Sleeper_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17786]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17787]
				subType=enum:0
				text=string:Wer ist der Schläfer? 
				name=string:DIA_BaalTaran_Sleeper_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17788]
		blockName=string:DIA_BaalTaran_Sleeper_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17789]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17790]
				subType=enum:0
				text=string:Der Schläfer ist unser Erlöser. Er hat sich uns vor 5 Jahren durch Y'Berion, unseren Meister, offenbart. 
				name=string:DIA_BaalTaran_Sleeper_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17791]
		blockName=string:DIA_BaalTaran_Sleeper_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17792]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17793]
				subType=enum:0
				text=string:Seitdem haben wir den drei Göttern abgeschworen. Und das solltest du auch tun. 
				name=string:DIA_BaalTaran_Sleeper_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17794]
		blockName=string:DIA_BaalTaran_Sleeper_05_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17795]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17796]
				subType=enum:0
				text=string:Die Götter haben es zugelassen, dass wir in dieses Gefängnis verbannt wurden. Der Schläfer aber wird uns wieder in die Freiheit führen. 
				name=string:DIA_BaalTaran_Sleeper_05_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17797]
		blockName=string:DIA_BaalTaran_Lehre_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17798]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17799]
				subType=enum:0
				text=string:Erzähl mir von der Lehre des Schläfers. 
				name=string:DIA_BaalTaran_Lehre_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17800]
		blockName=string:DIA_BaalTaran_Lehre_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17801]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17802]
				subType=enum:0
				text=string:Der Schläfer ist ein mächtiges Wesen, vielleicht sogar mächtiger als Innos, der Gott des Lichts. 
				name=string:DIA_BaalTaran_Lehre_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17803]
		blockName=string:DIA_BaalTaran_Lehre_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17804]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17805]
				subType=enum:0
				text=string:Im Geiste ist er mit uns. Aber seine Macht ist noch begrenzt. Er schläft einen tausendjährigen Schlaf, aber die Zeit seines Erwachens ist nahe. 
				name=string:DIA_BaalTaran_Lehre_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17806]
		blockName=string:DIA_BaalTaran_Lehre_05_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17807]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17808]
				subType=enum:0
				text=string:Uns hat er auserwählt, sein Erwachen zu bezeugen und seine Lehre zu verbreiten. 
				name=string:DIA_BaalTaran_Lehre_05_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17809]
		blockName=string:DIA_BaalTaran_Lehre_05_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17810]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17811]
				subType=enum:0
				text=string:Denn höre, diejenigen, die sich dem Schläfer anschließen, deren Seelen werden unsterblich, aber die, die ihm entsagen oder sich gar gegen ihn stellen, werden vernichtet werden. 
				name=string:DIA_BaalTaran_Lehre_05_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17812]
		blockName=string:DIA_BaalTaran_Lehre_05_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17813]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17814]
				subType=enum:0
				text=string:Aber der Schläfer ist barmherzig und gibt allen Zweiflern einen Beweis seiner Macht. 
				name=string:DIA_BaalTaran_Lehre_05_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17815]
		blockName=string:DIA_BaalTaran_Freiheit_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17816]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17817]
				subType=enum:0
				text=string:Wie soll der Weg in die Freiheit aussehen? 
				name=string:DIA_BaalTaran_Freiheit_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17818]
		blockName=string:DIA_BaalTaran_Freiheit_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17819]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17820]
				subType=enum:0
				text=string:Wir bereiten eine große Anrufung des Schläfers vor, in der alle Novizen zusammen den Kontakt herstellen. 
				name=string:DIA_BaalTaran_Freiheit_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17821]
		blockName=string:DIA_BaalTaran_Freiheit_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17822]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17823]
				subType=enum:0
				text=string:In dieser Anrufung wird uns der Schläfer den Weg in die Freiheit offenbaren. 
				name=string:DIA_BaalTaran_Freiheit_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17824]
		blockName=string:DIA_BaalTaran_Beweis_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17825]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17826]
				subType=enum:0
				text=string:Wie beweist euch euer Gott seine Macht? 
				name=string:DIA_BaalTaran_Beweis_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17827]
		blockName=string:DIA_BaalTaran_Beweis_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17828]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17829]
				subType=enum:0
				text=string:Er spricht zu uns durch Visionen. Wer seine Stimme gehört hat, zweifelt nicht mehr an seiner Macht. 
				name=string:DIA_BaalTaran_Beweis_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17830]
		blockName=string:DIA_BaalTaran_Beweis_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17831]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17832]
				subType=enum:0
				text=string:Wenn man das Kraut, das im Sumpf wächst, einnimmt, ist man dem Schläfer nahe und kann ihn hören. Manche sehen auch Bilder. 
				name=string:DIA_BaalTaran_Beweis_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17833]
		blockName=string:DIA_BaalTaran_Beweis_15_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17834]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17835]
				subType=enum:0
				text=string:Verstehe. 
				name=string:DIA_BaalTaran_Beweis_15_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17836]
		blockName=string:DIA_BaalTaran_WayToST_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17837]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17838]
				subType=enum:0
				text=string:Wie komme ich zum Lager der Bruderschaft? 
				name=string:DIA_BaalTaran_WayToST_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17839]
		blockName=string:DIA_BaalTaran_WayToST_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17840]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17841]
				subType=enum:0
				text=string:Ich bin gerade beschäftigt. Aber Baal Parvez müsste sich auch hier im Lager befinden. 
				name=string:DIA_BaalTaran_WayToST_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17842]
		blockName=string:DIA_BaalTaran_WayToST_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17843]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17844]
				subType=enum:0
				text=string:Er ist in der Nähe des Marktplatzes, auf der andern Seite der Burg. Ich glaube, er hat dort sogar 'ne eigene Hütte. 
				name=string:DIA_BaalTaran_WayToST_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17845]
		blockName=string:DIA_BaalTaran_IntoCastle_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17846]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17847]
				subType=enum:0
				text=string:Ich muss in die Burg. Kannst du mir dabei helfen? 
				name=string:DIA_BaalTaran_IntoCastle_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17848]
		blockName=string:DIA_BaalTaran_IntoCastle_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17849]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17850]
				subType=enum:0
				text=string:Ich fürchte nicht, Bruder. Die einzigen von uns, die die Burg betreten dürfen, sind diejenigen, die Gomez das Kraut liefern. 
				name=string:DIA_BaalTaran_IntoCastle_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17851]
		blockName=string:DIA_BaalTaran_IntoCastle_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17852]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17853]
				subType=enum:0
				text=string:Wir ernten Sumpfkraut und verkaufen einen Teil davon ans Alte Lager, zum Austausch gegen Waren aus der Außenwelt. 
				name=string:DIA_BaalTaran_IntoCastle_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17854]
		blockName=string:DIA_BaalTaran_IntoCastle_05_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17855]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17856]
				subType=enum:0
				text=string:Wenn du einer der Kuriere wärst, die für Cor Kalom arbeiten, könntest du in die Burg kommen. Aber dazu müsstest du natürlich einer von uns sein. 
				name=string:DIA_BaalTaran_IntoCastle_05_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17857]
		blockName=string:DIA_BaalTaran_IntoCastle_05_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17858]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17859]
				subType=enum:0
				text=string:Was willst du überhaupt in der Burg? 
				name=string:DIA_BaalTaran_IntoCastle_05_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17860]
		blockName=string:DIA_BaalTaran_IntoCastle_MeetSomeone_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17861]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17862]
				subType=enum:0
				text=string:Ich will dort jemanden treffen. 
				name=string:DIA_BaalTaran_IntoCastle_MeetSomeone_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17863]
		blockName=string:DIA_BaalTaran_IntoCastle_MeetSomeone_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17864]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17865]
				subType=enum:0
				text=string:So? Wen? Und wieso wartest du nicht einfach, bis er rauskommt? 
				name=string:DIA_BaalTaran_IntoCastle_MeetSomeone_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17866]
		blockName=string:DIA_BaalTaran_IntoCastle_Letter_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17867]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17868]
				subType=enum:0
				text=string:Ich habe einen Brief für den obersten Feuermagier. 
				name=string:DIA_BaalTaran_IntoCastle_Letter_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17869]
		blockName=string:DIA_BaalTaran_IntoCastle_Letter_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17870]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17871]
				subType=enum:0
				text=string:Ach? Und woher hast du den? Den haben dir die Magier gegeben, bevor sie dich hier reingeworfen haben, richtig? 
				name=string:DIA_BaalTaran_IntoCastle_Letter_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17872]
		blockName=string:DIA_BaalTaran_IntoCastle_Letter_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17873]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17874]
				subType=enum:0
				text=string:Du bist ganz schön dumm, das so rum zu erzählen. Aber keine Sorge - ich behalt's für mich. 
				name=string:DIA_BaalTaran_IntoCastle_Letter_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17875]
		blockName=string:DIA_BaalTaran_IntoCastle_Letter_05_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17876]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17877]
				subType=enum:0
				text=string:Nur tu dir selber einen Gefallen und erzähl es keinem anderen, ja? Die Leute hier schneiden dir für so was die Kehle durch! 
				name=string:DIA_BaalTaran_IntoCastle_Letter_05_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17878]
		blockName=string:DIA_BaalTaran_IntoCastle_Letter_05_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17879]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17880]
				subType=enum:0
				text=string:Die Belohnung, die es für eine Nachricht aus der Außenwelt gibt, will jeder gerne kassieren. 
				name=string:DIA_BaalTaran_IntoCastle_Letter_05_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17881]
		blockName=string:DIA_BaalTaran_IntoCastle_Mage_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17882]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17883]
				subType=enum:0
				text=string:Es ist einer der Magier. 
				name=string:DIA_BaalTaran_IntoCastle_Mage_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17884]
		blockName=string:DIA_BaalTaran_IntoCastle_Mage_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17885]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17886]
				subType=enum:0
				text=string:Oh! Tja, die kommen wirklich nie raus. Da musst du schon in die Burg rein. 
				name=string:DIA_BaalTaran_IntoCastle_Mage_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17887]
		blockName=string:DIA_BaalTaran_IntoCastle_Mage_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17888]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17889]
				subType=enum:0
				text=string:Was willst du denn von denen? Magische Runen kaufen? Oder Tränke? 
				name=string:DIA_BaalTaran_IntoCastle_Mage_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17890]
