	[]
	[% zCCSBlock 0 12970]
		blockName=string:GUR_1202_CorAngar_DEATH_Info_08_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12971]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12972]
				subType=enum:0
				text=string:Er ist mit einigen Templern völlig zornerfüllt aufgebrochen, um einen eigenen Weg zum Schläfer zu finden. 
				name=string:GUR_1202_CorAngar_DEATH_Info_08_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12973]
		blockName=string:GUR_1202_CorAngar_DEATH_Info_15_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12974]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12975]
				subType=enum:0
				text=string:Verdammt, wie sollen wir jetzt an den Fokus kommen? 
				name=string:GUR_1202_CorAngar_DEATH_Info_15_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12976]
		blockName=string:GUR_1202_CorAngar_DEATH_Info_08_09
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12977]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12978]
				subType=enum:0
				text=string:Ich schätze, er ließ Buch und Fokus in seinem Labor zurück. Hier ist der Schlüssel für seine Truhe. 
				name=string:GUR_1202_CorAngar_DEATH_Info_08_09.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12979]
		blockName=string:DIA_BaalTondral_FanaticTeacher_13_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12980]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12981]
				subType=enum:0
				text=string:Du siehst nicht aus wie einer, der sein Leben bereits unter die Knechtschaft der Erzbarone gestellt hat. 
				name=string:DIA_BaalTondral_FanaticTeacher_13_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12982]
		blockName=string:DIA_BaalTondral_FanaticTeacher_13_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12983]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12984]
				subType=enum:0
				text=string:Du siehst aus wie ein Suchender. Du siehst aus wie einer, der den wahren Glauben sucht. Spürst du nicht das Feuer in dir, das dir nachts den Schlaf raubt? 
				name=string:DIA_BaalTondral_FanaticTeacher_13_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12985]
		blockName=string:DIA_BaalTondral_FanaticTeacher_13_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12986]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12987]
				subType=enum:0
				text=string:Ich sehe, dass du an dem zweifelst, was dir die Knechte der Erzbarone als wahr und gerecht verkaufen wollen. Du weißt auch warum. Weil es Lügen sind! 
				name=string:DIA_BaalTondral_FanaticTeacher_13_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12988]
		blockName=string:DIA_BaalTondral_FanaticTeacher_13_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12989]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12990]
				subType=enum:0
				text=string:Es sind Lügen, mit denen sie den Geist derer beherrschen, die schwach sind. Dein Glaube ist stärker als ihre Lügen! 
				name=string:DIA_BaalTondral_FanaticTeacher_13_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12991]
		blockName=string:DIA_BaalTondral_FanaticTeacher_13_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12992]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12993]
				subType=enum:0
				text=string:Fühlst du nicht jeden Tag den Wunsch nach Freiheit? Fühlst du wie er jeden Tag wächst? Er lenkt deinen Geist. Lass es geschehen! 
				name=string:DIA_BaalTondral_FanaticTeacher_13_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12994]
		blockName=string:DIA_BaalTondral_FanaticTeacher_13_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12995]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12996]
				subType=enum:0
				text=string:Weißt du, was du tust, wenn du ihn beherrscht? Du verleugnest deine Seele. Du verleugnest dich selbst! Hör auf, im Schatten deiner Ängste zu leben. 
				name=string:DIA_BaalTondral_FanaticTeacher_13_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12997]
		blockName=string:DIA_BaalTondral_FanaticTeacher_13_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12998]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12999]
				subType=enum:0
				text=string:Mach dich frei. Verstehe! Der Schläfer WIRD erwachen, er wird dir die Freiheit bringen, und das ganze ungläubige Pack wegschwemmen. 
				name=string:DIA_BaalTondral_FanaticTeacher_13_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13000]
		blockName=string:DIA_BaalTondral_FanaticTeacher_13_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13001]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13002]
				subType=enum:0
				text=string:Wie Unrat, der vom Regen weggespült wird, werden sie vom Antlitz dieser Welt verschwinden und sie werden klagen und jammern und bereuen, aber dann ist es zu spät. 
				name=string:DIA_BaalTondral_FanaticTeacher_13_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13003]
		blockName=string:DIA_BaalTondral_FanaticTeacher_13_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13004]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13005]
				subType=enum:0
				text=string:Dann werden sie zahlen. Mit ihrem Blut. Und ihrem Leben. 
				name=string:DIA_BaalTondral_FanaticTeacher_13_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13006]
		blockName=string:DIA_BaalTondral_YouTalked_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13007]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13008]
				subType=enum:0
				text=string:Hey! Du hast mich angesprochen. Heißt das, ich kann jetzt mit dir reden? 
				name=string:DIA_BaalTondral_YouTalked_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13009]
		blockName=string:DIA_BaalTondral_YouTalked_13_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13010]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13011]
				subType=enum:0
				text=string:Ich sehe, du hast dich mit unseren Regeln vertraut gemacht. Das ist gut. 
				name=string:DIA_BaalTondral_YouTalked_13_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13012]
		blockName=string:DIA_BaalTondral_YouTalked_13_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13013]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13014]
				subType=enum:0
				text=string:Ich spüre große geistige Kraft in dir! Du kannst schon bald ein Mitglied unserer Gemeinschaft sein. 
				name=string:DIA_BaalTondral_YouTalked_13_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13015]
		blockName=string:DIA_BaalTondral_YouTalked_13_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13016]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13017]
				subType=enum:0
				text=string:Ich habe dich angesprochen weil ich dir Gelegenheit geben will, uns deine Loyalität zu beweisen. 
				name=string:DIA_BaalTondral_YouTalked_13_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13018]
		blockName=string:DIA_BaalTondral_Loyality_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13019]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13020]
				subType=enum:0
				text=string:Wie kann ich dir meine Loyalität beweisen? 
				name=string:DIA_BaalTondral_Loyality_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13021]
		blockName=string:DIA_BaalTondral_Loyality_13_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13022]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13023]
				subType=enum:0
				text=string:Was wir am dringendsten brauchen, sind neue Seelen, die sich unserer Gemeinschaft anschließen. 
				name=string:DIA_BaalTondral_Loyality_13_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13024]
		blockName=string:DIA_BaalTondral_Loyality_13_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13025]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13026]
				subType=enum:0
				text=string:Wenn du dafür sorgst, dass ein weiterer Mann unserem Lager beitritt, hast du deinen Willen, unsere Gemeinschaft zu bereichern, unter Beweis gestellt. 
				name=string:DIA_BaalTondral_Loyality_13_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13027]
		blockName=string:DIA_BaalTondral_NewMember_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13028]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13029]
				subType=enum:0
				text=string:Wo kann ich neue Leute für die Bruderschaft anwerben? 
				name=string:DIA_BaalTondral_NewMember_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13030]
		blockName=string:DIA_BaalTondral_NewMember_13_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13031]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13032]
				subType=enum:0
				text=string:Das alte Lager ist ein guter Ort dafür. Denn viele, die dort leben, sind der Angst und der Lügen überdrüssig, sie müssen nur ein wenig unterstützt werden. 
				name=string:DIA_BaalTondral_NewMember_13_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13033]
		blockName=string:DIA_BaalTondral_NewMember_13_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13034]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13035]
				subType=enum:0
				text=string:Im Neuen Lager wirst du wahrscheinlich weniger Glück haben. Die Männer, die dort leben sind Barbaren. 
				name=string:DIA_BaalTondral_NewMember_13_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13036]
		blockName=string:DIA_BaalTondral_NewMember2_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13037]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13038]
				subType=enum:0
				text=string:Wieso braucht ihr so dringend neue Leute? 
				name=string:DIA_BaalTondral_NewMember2_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13039]
		blockName=string:DIA_BaalTondral_NewMember2_13_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13040]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13041]
				subType=enum:0
				text=string:Der Schläfer hat unserem Meister offenbart, dass er eine wichtige Nachricht für uns hat. 
				name=string:DIA_BaalTondral_NewMember2_13_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13042]
		blockName=string:DIA_BaalTondral_NewMember2_13_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13043]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13044]
				subType=enum:0
				text=string:Aber er ist noch nicht zu seiner vollen Stärke erwacht, und so braucht er UNSERE Kraft, um mit uns in Kontakt treten zu können. 
				name=string:DIA_BaalTondral_NewMember2_13_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13045]
		blockName=string:DIA_BaalTondral_NewMember2_13_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13046]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13047]
				subType=enum:0
				text=string:Deswegen bereiten wir eine große Anrufung vor und je mehr Männer daran teilnehmen, umso eher werden wir Erfolg haben. 
				name=string:DIA_BaalTondral_NewMember2_13_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13048]
		blockName=string:DIA_BaalTondral_NewMember2_13_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13049]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13050]
				subType=enum:0
				text=string:Wenn du mehr darüber erfahren willst, geh zum großen Platz vor dem Tempelberg. Aber störe nicht den Meister bei der Meditation! 
				name=string:DIA_BaalTondral_NewMember2_13_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13051]
		blockName=string:DIA_BaalTondral_DustySuccess_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13052]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13053]
