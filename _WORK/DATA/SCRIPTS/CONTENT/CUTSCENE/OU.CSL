		[]
	[]
	[% zCCSBlock 0 27841]
		blockName=string:DIA_Mud_FirstEXIT_Verpiss_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27842]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27843]
				subType=enum:0
				text=string:Bist heute nicht so gut drauf, was? Ich glaub', ich komm' mal besser mit ... 
				name=string:DIA_Mud_FirstEXIT_Verpiss_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27844]
		blockName=string:DIA_Mud_FirstEXIT_Komm_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27845]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27846]
