	[% zCCSBlock 0 27856]
		blockName=string:DIA_Mud_GetLost_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27857]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27858]
				subType=enum:0
				text=string:Verpiss dich! 
				name=string:DIA_Mud_GetLost_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27859]
		blockName=string:DIA_Mud_GetLost_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27860]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27861]
				subType=enum:0
				text=string:Willst 'nen Moment allein sein, hm? Kann ich verstehen ... Ich halt mich im Hintergrund ... 
				name=string:DIA_Mud_GetLost_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27862]
		blockName=string:DIA_Mud_GetLost_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27863]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27864]
				subType=enum:0
				text=string:Hey, Mann, du hast mich geschlagen! Was sollte das? 
				name=string:DIA_Mud_GetLost_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27865]
		blockName=string:DIA_Mud_GetLost_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27866]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27867]
				subType=enum:0
				text=string:Ich schlag' dich gleich noch mal! Ich steh' auf Schlagen! 
				name=string:DIA_Mud_GetLost_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27868]
		blockName=string:DIA_Mud_GetLost_07_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27869]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27870]
				subType=enum:0
				text=string:Mann, was bist DU denn für'n Freak! Mit dir will ich nichts mehr zu tun haben! 
				name=string:DIA_Mud_GetLost_07_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27871]
		blockName=string:DIA_Mud_Nerve_0_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27872]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27873]
				subType=enum:0
				text=string:Sag mal, wohin geht es eigentlich? Oder ist das eine Überraschung für mich? Ich mag Überraschungen. 
				name=string:DIA_Mud_Nerve_0_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27874]
		blockName=string:DIA_Mud_Nerve_1_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27875]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27876]
				subType=enum:0
				text=string:Lass dich von mir nicht stören. Ich bleib' einfach bei dir, keine Sorge. 
				name=string:DIA_Mud_Nerve_1_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27877]
		blockName=string:DIA_Mud_Nerve_2_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27878]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27879]
				subType=enum:0
				text=string:Hast du eigentlich keinen Plan, wo du hinwillst? Suchst du ein stilles Plätzchen? Wo wir ein bisschen reden können und so? 
				name=string:DIA_Mud_Nerve_2_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27880]
		blockName=string:DIA_Mud_Nerve_3_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27881]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27882]
				subType=enum:0
				text=string:Hast du eigentlich keinen Plan, wo du hinwillst? Suchst du ein stilles Plätzchen? Wo wir ein bisschen reden können und so? 
				name=string:DIA_Mud_Nerve_3_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27883]
		blockName=string:DIA_Mud_Nerve_4_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27884]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27885]
				subType=enum:0
				text=string:Kann es sein, dass du im Kreis läufst? Ich meine, ich weiß ja nicht, wo du hin willst ... 
				name=string:DIA_Mud_Nerve_4_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27886]
		blockName=string:DIA_Mud_Nerve_5_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27887]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27888]
				subType=enum:0
				text=string:Das macht richtig Spaß mit dir. Schön, dass du mich mitnimmst. Das können wir öfter mal machen. Und heute ist ja auch noch viel Zeit. 
				name=string:DIA_Mud_Nerve_5_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27889]
		blockName=string:DIA_Mud_Nerve_6_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27890]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27891]
				subType=enum:0
				text=string:Ich hab' mich schon richtig an dich gewöhnt. Wir werden sicher eine Menge Spaß miteinander haben. Nicht wahr? Wo schläfst du eigentlich? Du kannst gerne zu mir kommen. 
				name=string:DIA_Mud_Nerve_6_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27892]
		blockName=string:DIA_Mud_Nerve_7_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27893]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27894]
				subType=enum:0
				text=string:Wegen was haben sie dich eigentlich hier reingeworfen? Hast du auch einen Faible für Tiere? Das haben sie ja gar nicht gerne. Da haben sie mich eingebuchtet. 
				name=string:DIA_Mud_Nerve_7_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27895]
		blockName=string:DIA_Mud_Nerve_8_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27896]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27897]
				subType=enum:0
				text=string:Es gibt da ein paar Leute, die versuchen ständig, mich zu verprügeln. Fiese Typen aus dem Neuen Lager. Richtige Brutalos. Ich werde ihnen beim nächsten Mal gleich sagen, dass sie es in Zukunft mir dir zu tun kriegen. Das wird sie sicher abschrecken. 
				name=string:DIA_Mud_Nerve_8_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27898]
		blockName=string:DIA_Mud_Nerve_9_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27899]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27900]
				subType=enum:0
				text=string:Diese fiesen Schweine aus dem Neuen Lager werden staunen, wenn sie erfahren, dass ich einen Beschützer habe. Die werden sich in die Hosen machen. Das wird ein Spaß. Sonst machen sie ja mit allen kurzen Prozess, aber jetzt werden sie sich das zweimal überlegen. 
				name=string:DIA_Mud_Nerve_9_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27901]
		blockName=string:DIA_Mud_Nerve_10_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27902]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27903]
				subType=enum:0
				text=string:Weißt du was? Ich tu dir einen Gefallen. Ich werde ein gutes Wort für dich einlegen. Dann nehmen sie dich vielleicht in das Lager auf. Dann können wir uns jeden Tag sehen. 
				name=string:DIA_Mud_Nerve_10_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27904]
		blockName=string:DIA_Mud_Nerve_11_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27905]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27906]
				subType=enum:0
				text=string:Manche halten mich ja für einen durchgeknallten Spinner. Aber jemand wie du würde sich hier ja niemals mit einem Spinner sehen lassen. So was spricht sich ja rum. Also werden sie einsehen, dass sie sich in mir getäuscht haben. 
				name=string:DIA_Mud_Nerve_11_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27907]
		blockName=string:DIA_Mud_Nerve_12_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27908]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27909]
				subType=enum:0
				text=string:Du bist mein bester Freund hier drinnen. Die anderen wollen mich immer nur loswerden. Ich bin dir echt dankbar. 
				name=string:DIA_Mud_Nerve_12_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27910]
		blockName=string:DIA_Mud_Nerve_13_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27911]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27912]
				subType=enum:0
				text=string:Hör mal, die Rennerei wird langsam anstrengend. Wollen wir uns nicht irgendwo ein kuscheliges Plätzchen suchen und es uns gemütlich machen? 
				name=string:DIA_Mud_Nerve_13_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27913]
		blockName=string:DIA_Mud_Nerve_14_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27914]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27915]
				subType=enum:0
				text=string:Du bist wie ein großer Bruder für mich. Ich hatte nie einen großen Bruder. Auch keinen kleinen. Meine Eltern wollten mich nicht behalten. Ich bin sicher, das fiel ihnen nicht leicht. 
				name=string:DIA_Mud_Nerve_14_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27916]
		blockName=string:DIA_Mud_Nerve_14_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27917]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27918]
				subType=enum:0
				text=string:Bestimmt nicht. 
				name=string:DIA_Mud_Nerve_14_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27919]
		blockName=string:DIA_Mud_Nerve_15_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27920]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27921]
				subType=enum:0
				text=string:Wo kommst du denn her? Du freust dich sicher auch so, dass du jetzt endlich jemanden gefunden hast, der dich versteht. Ich komme aus Khorinis. Warst du schon mal dort? Ist ja eigentlich auch egal. 
				name=string:DIA_Mud_Nerve_15_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27922]
		blockName=string:DIA_Mud_Nerve_16_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27923]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27924]
				subType=enum:0
				text=string:Hast du was zu essen für mich? Da ich ja so was wie Besuch bin, fände ich es toll, wenn du mir was geben würdest. Das gehört sich so. Das haben mir meine Freunde in Khorinis beigebracht. Die hatten mich echt gerne. 
				name=string:DIA_Mud_Nerve_16_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27925]
		blockName=string:DIA_Mud_Nerve_17_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27926]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27927]
				subType=enum:0
				text=string:Glaubst du, dass uns mal jemand von außerhalb besucht? Wohl kaum, was, weil die kommen ja dann nicht mehr raus, oder? 
				name=string:DIA_Mud_Nerve_17_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27928]
		blockName=string:DIA_Mud_Nerve_18_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27929]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27930]
				subType=enum:0
				text=string:Jetzt, wo du bei mir bist, können wir ja mal einigen Arschlöchern aufs Maul hauen. Die haben mich jahrelang getriezt. Wenn ich einen von denen sehe, dann sag' ich ihnen die Meinung. 
				name=string:DIA_Mud_Nerve_18_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27931]
		blockName=string:DIA_Mud_Nerve_19_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27932]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27933]
				subType=enum:0
				text=string:Zusammen sind wir echt unschlagbar. Wir könnten das Alte Lager übernehmen. Mit den Erzbaronen werden wir schon fertig, wir sind ja zu zweit. Ich werde mir etwas ausdenken. 
				name=string:DIA_Mud_Nerve_19_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27934]
		blockName=string:DIA_Grim_Falle_06_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27935]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27936]
				subType=enum:0
				text=string:Hey, du! Du bist doch der Typ, der hier im Lager aufgenommen werden will? 
				name=string:DIA_Grim_Falle_06_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27937]
		blockName=string:DIA_Grim_Falle_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27938]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27939]
				subType=enum:0
				text=string:Und wenn es so wäre? 
				name=string:DIA_Grim_Falle_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27940]
		blockName=string:DIA_Grim_Falle_06_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27941]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27942]
				subType=enum:0
				text=string:Ich könnte dir dabei helfen ... 
				name=string:DIA_Grim_Falle_06_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27943]
		blockName=string:DIA_Grim_Falle_06_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27944]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27945]
				subType=enum:0
				text=string:Draußen vorm Lager sitzen zwei Typen rum, von denen einer die Erzbarone bestohlen hat. Es ist ein wertvolles Amulett, das mit dem letzten Konvoi gekommen sein sollte. 
				name=string:DIA_Grim_Falle_06_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27946]
		blockName=string:DIA_Grim_Falle_06_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27947]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27948]
				subType=enum:0
				text=string:Einer von ihnen müsste das Amulett noch haben. Wenn wir beide auf sie draufgehen, schaffen wir sie - alleine kann ich's nicht machen. Was sagst du? 
				name=string:DIA_Grim_Falle_06_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27949]
		blockName=string:DIA_Grim_Falle_Deny_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27950]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27951]
				subType=enum:0
				text=string:Kein Interesse - such dir einen anderen. 
				name=string:DIA_Grim_Falle_Deny_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27952]
		blockName=string:DIA_Grim_Falle_Deny_06_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27953]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27954]
				subType=enum:0
				text=string:Mit der Einstellung kommst du hier nicht weit - sag mir Bescheid, falls du's dir doch noch überlegen solltest! 
				name=string:DIA_Grim_Falle_Deny_06_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27955]
		blockName=string:DIA_Grim_Falle_HowShare_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27956]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27957]
				subType=enum:0
				text=string:Was passiert, wenn wir das Amulett haben? 
				name=string:DIA_Grim_Falle_HowShare_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27958]
		blockName=string:DIA_Grim_Falle_HowShare_06_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27959]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27960]
				subType=enum:0
				text=string:Ganz einfach! Ich geb's zurück und kassier die Belohnung - dabei sag' ich, dass du mir geholfen hast. Damit hast du bei Gomez 'nen Stein in Brett. 
				name=string:DIA_Grim_Falle_HowShare_06_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27961]
		blockName=string:DIA_Grim_Falle_HalfHalf_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27962]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27963]
				subType=enum:0
				text=string:Nein! Wir machen halbe-halbe! Oder du kannst dich alleine mit den Typen anlegen! 
