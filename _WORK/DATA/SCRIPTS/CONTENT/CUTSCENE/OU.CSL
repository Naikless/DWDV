	[]
	[% zCCSBlock 0 26317]
		blockName=string:TPL_1402_GorNaToth_TRAIN_Info_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26318]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26319]
				subType=enum:0
				text=string:Ich will besser mit Einhandwaffen umgehen können. 
				name=string:TPL_1402_GorNaToth_TRAIN_Info_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26320]
		blockName=string:TPL_1402_GorNaToth_TRAIN_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26321]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26322]
				subType=enum:0
				text=string:Gute Entscheidung! Um deine Technik zu verbessern, musst du erst einmal lernen, deine Waffe richtig zu halten. 
				name=string:TPL_1402_GorNaToth_TRAIN_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26323]
		blockName=string:TPL_1402_GorNaToth_TRAIN_11_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26324]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26325]
				subType=enum:0
				text=string:Anfänger neigen dazu, Einhandwaffen mit beiden Händen zu halten. Mit so was fängst du am besten gar nicht erst an, das behindert dich nur. 
				name=string:TPL_1402_GorNaToth_TRAIN_11_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26326]
		blockName=string:TPL_1402_GorNaToth_TRAIN_11_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26327]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26328]
				subType=enum:0
				text=string:Halt die Waffe mit einer Hand, Klinge nach oben, und dann immer schön schwingen lassen. 
				name=string:TPL_1402_GorNaToth_TRAIN_11_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26329]
		blockName=string:TPL_1402_GorNaToth_TRAIN_11_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26330]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26331]
				subType=enum:0
				text=string:Du musst lernen, den Schwung deiner Waffe mit deinen Bewegungen in Einklang zu bringen, dann kannst du schneller zuschlagen. 
				name=string:TPL_1402_GorNaToth_TRAIN_11_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26332]
		blockName=string:TPL_1402_GorNaToth_TRAIN_11_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26333]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26334]
				subType=enum:0
				text=string:Wenn du dich an meine Anweisungen hältst, wirst du in Zukunft eleganter und vor allem schneller kämpfen. 
				name=string:TPL_1402_GorNaToth_TRAIN_11_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26335]
		blockName=string:TPL_1402_GorNaToth_TRAIN_11_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26336]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26337]
				subType=enum:0
				text=string:Ach, und noch was: Manche Treffer richten mehr Schaden an als andere! Als Anfänger hast du nur eine geringe Chance, solche kritischen Treffer zu landen. 
				name=string:TPL_1402_GorNaToth_TRAIN_11_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26338]
		blockName=string:TPL_1402_GorNaToth_TRAIN_11_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26339]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26340]
				subType=enum:0
				text=string:Aber je weiter du fortschreitest, desto besser wird diese Fähigkeit. 
				name=string:TPL_1402_GorNaToth_TRAIN_11_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26341]
		blockName=string:TPL_1402_GorNaToth_TRAINAGAIN_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26342]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26343]
				subType=enum:0
				text=string:Zeige mir, wie ich im Umgang mit Waffen noch besser werden kann. 
				name=string:TPL_1402_GorNaToth_TRAINAGAIN_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26344]
		blockName=string:TPL_1402_GorNaToth_TRAINAGAIN_Info_11_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26345]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26346]
				subType=enum:0
				text=string:Na gut, die Grundkenntnisse hast du ja schon. Wenn du die Waffe gesenkt hältst, dann kannst du direkt mehr Wucht in deinen ersten Schlag legen. 
				name=string:TPL_1402_GorNaToth_TRAINAGAIN_Info_11_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26347]
		blockName=string:TPL_1402_GorNaToth_TRAINAGAIN_Info_11_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26348]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26349]
				subType=enum:0
				text=string:Nutze den Schwung, weißt du noch? Gut, der nächste Schritt ist, dass du deinen Körper mehr einsetzt. Wenn du zweimal geschlagen hast, drehe dich um die eigene Achse, damit überraschst du den Gegner und bist in einer optimalen Position. 
				name=string:TPL_1402_GorNaToth_TRAINAGAIN_Info_11_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26350]
		blockName=string:TPL_1402_GorNaToth_TRAINAGAIN_Info_11_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26351]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26352]
				subType=enum:0
				text=string:Dann ziehst du deine Klinge noch einmal quer von rechts nach links . 
				name=string:TPL_1402_GorNaToth_TRAINAGAIN_Info_11_04.WAV
			[]
		[]
	[]
