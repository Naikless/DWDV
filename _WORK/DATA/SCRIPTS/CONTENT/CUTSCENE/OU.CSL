		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 23240]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 23241]
				subType=enum:0
				text=string:Jeder, der hier ankommt, wird vor eine Wahl gestellt. Es gibt drei Lager in der Kolonie, und einem davon wirst du dich anschließen müssen. 
				name=string:Info_Diego_WarumGeholfen_11_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 23242]
		blockName=string:Info_Diego_WarumGeholfen_11_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 23243]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 23244]
				subType=enum:0
				text=string:Ich bin hier, um Neuen wie dir klarzumachen, dass sie bei uns im alten Lager am besten aufgehoben sind. 
				name=string:Info_Diego_WarumGeholfen_11_06.WAV
			[]
		[]
	[]
