		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17690]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17691]
				subType=enum:0
				text=string:Oder du erreichst die höchsten Ehren und wirst zum Templer berufen. Nur die besten Kämpfer unter uns dürfen diese Position bekleiden. 
				name=string:DIA_BaalParvez_MyAdvantage_10_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17692]
