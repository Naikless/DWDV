			[]
		[]
	[]
	[% zCCSBlock 0 17710]
		blockName=string:DIA_BaalParvez_Sleeper_10_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17711]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17712]
				subType=enum:0
				text=string:Alle Anhänger der Bruderschaft haben dem Glauben an die drei Götter abgeschworen. Wir bitten nur noch den Schläfer um Erlösung. 
				name=string:DIA_BaalParvez_Sleeper_10_03.WAV
