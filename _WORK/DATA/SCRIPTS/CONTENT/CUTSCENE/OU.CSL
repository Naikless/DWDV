		subBlock0=float:0
		[% zCCSAtomicBlock 0 26282]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26283]
				subType=enum:0
				text=string:Ich möchte eine mittelschwere Templer-Rüstung haben. 
				name=string:Info_GorNaToth_ARMOR_M_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26284]
		blockName=string:Info_GorNaToth_ARMOR_M_11_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26285]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26286]
				subType=enum:0
				text=string:Du bist noch nicht so weit! Bewähre dich erst als Templer, dann wirst dir auch das Recht zuteil, eine bessere Rüstung zu tragen. 
				name=string:Info_GorNaToth_ARMOR_M_11_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26287]
		blockName=string:Info_GorNaToth_ARMOR_M_11_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26288]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26289]
				subType=enum:0
				text=string:Sobald du in der Lage bist, eine ausreichend große Spende an unsere Gemeinschaft zu leisten, steht dir eine bessere Rüstung zur Verfügung! 
				name=string:Info_GorNaToth_ARMOR_M_11_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26290]
		blockName=string:Info_GorNaToth_ARMOR_M_11_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26291]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26292]
				subType=enum:0
				text=string:Nun, da du in der Lage bist, die erforderliche Spende zu leisten, sollst du fortan von einer neuen Rüstung geschützt werden. 
				name=string:Info_GorNaToth_ARMOR_M_11_04.WAV
			[]
		[]
	[]
