		[]
	[]
	[% zCCSBlock 0 13519]
		blockName=string:Gur_1208_BaalCadar_KREIS2_Info_02_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13520]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13521]
				subType=enum:0
				text=string:Sie arbeiteten ohne Unterlass, Tage, Wochen Monate. Und das Lager wuchs heran. Mehr Männer kamen dazu, die dem Ruf des Schläfers gefolgt waren. 
				name=string:Gur_1208_BaalCadar_KREIS2_Info_02_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 13522]
		blockName=string:Gur_1208_BaalCadar_KREIS2_Info_02_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13523]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13524]
				subType=enum:0
				text=string:Und zusammen versuchten sie, durch Gebet und Meditation den Schläfer auf spiritueller Ebene zu treffen. 
				name=string:Gur_1208_BaalCadar_KREIS2_Info_02_05.WAV
