			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24354]
				subType=enum:0
				text=string:Nein, aber du solltest wissen, was dich erwartet. Es läuft hier nicht so wie im Alten Lager und erst recht nicht wie bei den Sektenspinnern! 
				name=string:DIA_Jarvis_First_08_02.WAV
