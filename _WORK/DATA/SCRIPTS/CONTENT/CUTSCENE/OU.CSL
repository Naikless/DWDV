	[% zCCSBlock 0 22801]
		blockName=string:DIA_Lester_CampInfo_GIL_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22802]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22803]
				subType=enum:0
				text=string:Die Gurus bilden die oberste Gilde. Sie sind der Geist des Lagers, denn ihre Macht ist groß. Die Templer benutzen ihre geistige Stärke im Kampf. 
				name=string:DIA_Lester_CampInfo_GIL_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22804]
		blockName=string:DIA_Lester_CampInfo_GIL_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22805]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22806]
				subType=enum:0
				text=string:Sie haben einen unbezwingbaren Willen. Leg dich besser NIE mit ihnen an. Ich selber gehöre zu den Novizen. Wir beten zum Schläfer und verrichten die Arbeiten, die hier im Lager anfallen. 
				name=string:DIA_Lester_CampInfo_GIL_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22807]
		blockName=string:DIA_Lester_CampInfo_GIL_05_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22808]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22809]
				subType=enum:0
				text=string:Einige der Novizen werden als Gurus aufgenommen, aber dazu gehört eine viele Jahre lange Ausbildung. 
				name=string:DIA_Lester_CampInfo_GIL_05_03.WAV
			[]
		[]
	[]
