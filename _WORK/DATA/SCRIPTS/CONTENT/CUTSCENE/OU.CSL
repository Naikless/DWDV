		subBlock0=float:0
		[% zCCSAtomicBlock 0 9065]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 9066]
				subType=enum:0
				text=string:Wir stehen Tag und Nacht in der Mine. Wir stehen Tag und Nacht an den Toren des Lagers und passen auf, dass alle ruhig schlafen können. 
				name=string:GRD_200_Thorus_AUFNAHME_Info_09_05.WAV
