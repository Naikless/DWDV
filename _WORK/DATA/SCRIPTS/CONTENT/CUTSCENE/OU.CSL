		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22274]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22275]
				subType=enum:0
				text=string:Wenn du wieder im Neuen Lager bist, werden die Wassermagier dich sicherlich für deine Mühen belohnen. 
				name=string:DIA_Milten_NochEinBrief_02_02.WAV
