			[]
		[]
	[]
	[% zCCSBlock 0 17716]
		blockName=string:DIA_BaalParvez_Sleeper_10_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17717]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17718]
				subType=enum:0
