	[]
	[% zCCSBlock 0 22831]
		blockName=string:DIA_Lester_CampInfo_HERB_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22832]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22833]
				subType=enum:0
				text=string:Nun, das Kraut wächst im Sumpf. Es muss natürlich noch verarbeitet werden, damit man es rauchen kann. Das machen wir Novizen. 
				name=string:DIA_Lester_CampInfo_HERB_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22834]
		blockName=string:DIA_Lester_CampInfo_HERB_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22835]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22836]
				subType=enum:0
				text=string:Das Kraut hat eine beruhigende und entspannende Wirkung. Es hilft dir, dich auf das Wesentliche zu konzentrieren und dein Bewusstsein zu öffnen. 
				name=string:DIA_Lester_CampInfo_HERB_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22837]
		blockName=string:DIA_Lester_CampInfo_HERB_05_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22838]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22839]
				subType=enum:0
				text=string:Wir tauschen es gegen Waren aus den anderen Lagern und benutzen es, um neue Leute anzuwerben. 
				name=string:DIA_Lester_CampInfo_HERB_05_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22840]
		blockName=string:DIA_Lester_CampInfo_HERB_05_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22841]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22842]
				subType=enum:0
				text=string:Das führt natürlich dazu, dass viele Leute nur wegen des Krauts zu uns kommen. Na ja, immerhin helfen sie bei den anfallenden Arbeiten. 
				name=string:DIA_Lester_CampInfo_HERB_05_04.WAV
			[]
		[]
	[]
