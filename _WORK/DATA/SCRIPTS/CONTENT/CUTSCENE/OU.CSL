			[]
		[]
	[]
	[% zCCSBlock 0 24268]
		blockName=string:SLD_709_Cord_TRAIN_Info_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24269]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24270]
				subType=enum:0
				text=string:Ich will besser mit Einhandwaffen umgehen können. 
