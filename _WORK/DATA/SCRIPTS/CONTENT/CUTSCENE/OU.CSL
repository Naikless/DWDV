	[% zCCSBlock 0 27172]
		blockName=string:DIA_Guy_Hello_03_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27173]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27174]
