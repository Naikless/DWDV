	[% zCCSBlock 0 26311]
		blockName=string:TPL_1402_GorNaToth_Teach_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26312]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26313]
