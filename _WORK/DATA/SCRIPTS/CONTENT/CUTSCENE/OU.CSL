		subBlock0=float:0
		[% zCCSAtomicBlock 0 16850]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 16851]
				subType=enum:0
				text=string:Falls du wirklich vorhast, dich mit den Biestern anzulegen, sprich besser vorher mit einem der Templer, der Erfahrung hat. Zum Beispiel Gor Na Drak. 
				name=string:DIA_Caine_WoSekret_13_02.WAV
