		subBlock0=float:0
		[% zCCSAtomicBlock 0 15773]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 15774]
				subType=enum:0
				text=string:Doch der mächtigste Zauber des fünften Kreises ist die Welle des Todes. Solltest du in den Besitz dieser Rune gelangen, dann bist du im Besitz gefährlicher Macht. 
				name=string:KDW_600_Saturas_KREIS5_Info_14_04.WAV
			[]
		[]
	[]
