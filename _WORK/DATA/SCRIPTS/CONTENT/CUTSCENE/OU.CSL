				subType=enum:0
				text=string:Wenn du nichts dagegen hast, komme ich ein Stück mit. Du kannst sicher einen Freund brauchen. 
				name=string:DIA_Mud_FirstEXIT_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27838]
		blockName=string:DIA_Mud_FirstEXIT_Verpiss_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27839]
