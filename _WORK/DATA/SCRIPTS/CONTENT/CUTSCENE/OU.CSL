				name=string:GUR_1202_CorAngar_LaterTrainer_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12712]
		blockName=string:GUR_1202_CorAngar_LaterTrainer_08_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12713]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12714]
				subType=enum:0
				text=string:Du willst ein starker Kämpfer werden und bist auf der Suche nach jemandem, der dich unterweist. 
				name=string:GUR_1202_CorAngar_LaterTrainer_08_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12715]
		blockName=string:GUR_1202_CorAngar_LaterTrainer_08_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12716]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12717]
				subType=enum:0
				text=string:Das ist gut. Aber ich bin NICHT derjenige, den du suchst. 
				name=string:GUR_1202_CorAngar_LaterTrainer_08_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12718]
		blockName=string:GUR_1202_CorAngar_LaterTrainer_08_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12719]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12720]
				subType=enum:0
				text=string:Ich opfere meine Zeit nur denen, die dem heiligen Kreis der Templer angehören. 
				name=string:GUR_1202_CorAngar_LaterTrainer_08_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12721]
		blockName=string:GUR_1202_CorAngar_WieTempler_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12722]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12723]
				subType=enum:0
				text=string:Wie werde ich in den Kreis der Templer aufgenommen? 
				name=string:GUR_1202_CorAngar_WieTempler_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12724]
		blockName=string:GUR_1202_CorAngar_WieTempler_08_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12725]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12726]
				subType=enum:0
				text=string:Du musst eine Zeit lang als Novize gedient haben, um deinen Glauben an den Schläfer unter Beweis zu stellen. 
				name=string:GUR_1202_CorAngar_WieTempler_08_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12727]
		blockName=string:GUR_1202_CorAngar_WieTempler_08_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12728]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12729]
				subType=enum:0
				text=string:Dann kehre zu mir zurück, und ich werde dich unterweisen. 
				name=string:GUR_1202_CorAngar_WieTempler_08_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12730]
		blockName=string:GUR_1202_CorAngar_Teach_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12731]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12732]
				subType=enum:0
				text=string:Kannst du meine Stärke und mein Geschick trainieren? 
				name=string:GUR_1202_CorAngar_Teach_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12733]
		blockName=string:GUR_1202_CorAngar_Teach_08_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12734]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12735]
				subType=enum:0
				text=string:Kampfkraft und Wendigkeit sind die Vorraussetzungen für jeden Kämpfer. 
				name=string:GUR_1202_CorAngar_Teach_08_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12736]
		blockName=string:GUR_1202_CorAngar_WANNABETPL_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12737]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12738]
				subType=enum:0
				text=string:Ich möchte Templer werden! 
				name=string:GUR_1202_CorAngar_WANNABETPL_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12739]
		blockName=string:GUR_1202_CorAngar_WANNABETPL_Info_08_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12740]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12741]
				subType=enum:0
				text=string:Du bist noch nicht bereit. Wenn du erfahrener geworden bist, werde ich dich aufnehmen. 
				name=string:GUR_1202_CorAngar_WANNABETPL_Info_08_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12742]
		blockName=string:GUR_1202_CorAngar_WANNABETPL_Info_08_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12743]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12744]
				subType=enum:0
				text=string:Die Zeit ist gekommen, da du aufgenommen wirst. 
				name=string:GUR_1202_CorAngar_WANNABETPL_Info_08_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12745]
		blockName=string:GUR_1202_CorAngar_WANNABETPL_Info_08_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12746]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12747]
				subType=enum:0
				text=string:Du solltest eins wissen: Templer zu sein, bedeutet nicht nur gut kämpfen zu können und das Lager zu beschützen. Wir sind die auserwählte Wache des Schläfers. Die Hüter des Geistes. 
				name=string:GUR_1202_CorAngar_WANNABETPL_Info_08_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12748]
		blockName=string:GUR_1202_CorAngar_WANNABETPL_Info_08_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12749]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12750]
				subType=enum:0
				text=string:Wir nutzen unsere geistige und unsere körperliche Kraft zu gleichen Teilen. Nur ein Mann, dessen Körper und Geist sich in Einklang befinden, kann ein aufrechter Templer sein. 
				name=string:GUR_1202_CorAngar_WANNABETPL_Info_08_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12751]
		blockName=string:GUR_1202_CorAngar_WANNABETPL_Info_08_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12752]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12753]
				subType=enum:0
				text=string:Es freut mich, dass du es in so kurzer Zeit geschafft hast, so weit zu kommen, dass du nun bereit bist, einer der Unseren zu werden. 
				name=string:GUR_1202_CorAngar_WANNABETPL_Info_08_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12754]
		blockName=string:GUR_1202_CorAngar_WANNABETPL_Info_08_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12755]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12756]
				subType=enum:0
				text=string:Und vergiss nicht: ein konzentrierter Geist in einem gestählten Körper widersteht jeder Bedrohung. 
				name=string:GUR_1202_CorAngar_WANNABETPL_Info_08_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12757]
		blockName=string:GUR_1202_CorAngar_WANNABETPL_Info_08_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12758]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12759]
				subType=enum:0
				text=string:Gehe nun zu Gor Na Toth am Übungsplatz. Er wird dir deine neue Rüstung geben. 
				name=string:GUR_1202_CorAngar_WANNABETPL_Info_08_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12760]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND1_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12761]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12762]
				subType=enum:0
				text=string:Ich möchte den Umgang mit dem Zweihänder lernen. 
				name=string:GUR_1202_CorAngar_ZWEIHAND1_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12763]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND1_Info_08_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12764]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12765]
				subType=enum:0
				text=string:Der Kampf mit zweihändigen Waffen setzt nicht nur ein Mindestmaß an Stärke und Geschick voraus. 
				name=string:GUR_1202_CorAngar_ZWEIHAND1_Info_08_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12766]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND1_Info_08_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12767]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12768]
				subType=enum:0
				text=string:Um eine zweihändige Waffe zu führen, musst du nicht nur deinen Körper, sondern auch deinen Geist beherrschen. 
				name=string:GUR_1202_CorAngar_ZWEIHAND1_Info_08_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12769]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND1_Info_08_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12770]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12771]
				subType=enum:0
				text=string:Nur wenn du Körper und Geist ganz beherrscht, kannst du das Talent lernen. 
				name=string:GUR_1202_CorAngar_ZWEIHAND1_Info_08_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12772]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND1_Info_08_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12773]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12774]
				subType=enum:0
				text=string:Konzentration ist der Schlüssel. Körper und Geist verschmelzen in Konzentration zusammen. 
				name=string:GUR_1202_CorAngar_ZWEIHAND1_Info_08_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12775]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND2_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12776]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12777]
				subType=enum:0
				text=string:Ich möchte mehr über den zweihändigen Kampf lernen. 
				name=string:GUR_1202_CorAngar_ZWEIHAND2_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12778]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12779]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12780]
				subType=enum:0
				text=string:Den Tanz mit der Klinge beherrscht du nun. Jetzt werde ich dir die Meisterschaft des Kampfes zeigen. 
				name=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12781]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12782]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12783]
				subType=enum:0
				text=string:Nutze deine geistige Kraft, um die Bewegungen des Gegners vor dem Angriff zu erkennen. 
				name=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12784]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12785]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12786]
				subType=enum:0
				text=string:Zu wissen, wie dein Gegner kämpft, ist der halbe Sieg über ihn. 
				name=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12787]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12788]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12789]
				subType=enum:0
				text=string:Sei schnell, deine Bewegungen müssen überraschend und fließend sein, deine Schläge präzise und hart. 
				name=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12790]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12791]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12792]
				subType=enum:0
				text=string:Lerne den Schmerz zu ignorieren. Dein Wille ist es, der letztendlich über den Ausgang des Kampfes entscheidet. 
				name=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12793]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12794]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12795]
				subType=enum:0
				text=string:Bewahre die Ruhe. Nur wenn du ruhig und konzentriert in einen Kampf gehst, wirst du ihn auch überleben. 
				name=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12796]
		blockName=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12797]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12798]
				subType=enum:0
				text=string:Setze dein Talent mit Vorsicht ein. Und denke daran: Wer den Kampf fordert, kommt durch den Kampf um. 
				name=string:GUR_1202_CorAngar_ZWEIHAND2_Info_08_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12799]
		blockName=string:GUR_1202_CorAngar_SENDS_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12800]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12801]
				subType=enum:0
				text=string:Wie geht es jetzt weiter? 
				name=string:GUR_1202_CorAngar_SENDS_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12802]
		blockName=string:GUR_1202_CorAngar_SENDS_Info_08_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12803]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12804]
				subType=enum:0
				text=string:Die Vision war sehr verschwommen und gab mehr Rätsel auf, als dass sie uns Antworten brachte. 
				name=string:GUR_1202_CorAngar_SENDS_Info_08_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12805]
		blockName=string:GUR_1202_CorAngar_SENDS_Info_08_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12806]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12807]
				subType=enum:0
				text=string:Aber eines war klar. Mann konnte einen Ork in einer Höhle erkennen. Es gibt eine alte verlassene Kultstätte der Orks ganz hier in der Nähe. 
				name=string:GUR_1202_CorAngar_SENDS_Info_08_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12808]
		blockName=string:GUR_1202_CorAngar_SENDS_Info_08_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12809]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12810]
				subType=enum:0
				text=string:Früher haben dort die Orks ihre Verstorbenen beigesetzt, daher nennen wir dieses Höhlensystem den Ork-Friedhof. 
				name=string:GUR_1202_CorAngar_SENDS_Info_08_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12811]
		blockName=string:GUR_1202_CorAngar_SENDS_Info_08_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12812]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12813]
				subType=enum:0
				text=string:Der Guru Baal Lukor ist mit einigen Templern dorthin aufgebrochen. Vielleicht finden wir dort etwas, das uns hilft, die Vision besser zu verstehen. 
				name=string:GUR_1202_CorAngar_SENDS_Info_08_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12814]
		blockName=string:GUR_1202_CorAngar_SENDS_Info_08_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12815]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12816]
				subType=enum:0
				text=string:Ich brauche jemanden, der dort nach dem Rechten sieht und mir von der Situation berichtet. 
				name=string:GUR_1202_CorAngar_SENDS_Info_08_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12817]
		blockName=string:GUR_1202_CorAngar_SENDS_Info_15_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12818]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12819]
				subType=enum:0
				text=string:Warum gehst du nicht selber? Du bist doch der Anführer der Templer! 
				name=string:GUR_1202_CorAngar_SENDS_Info_15_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12820]
		blockName=string:GUR_1202_CorAngar_SENDS_Info_08_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12821]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12822]
				subType=enum:0
				text=string:Meine Aufgabe ist es, den Erleuchteten zu schützen. Ich bleibe hier bei Y'Berion und wache über ihn. 
				name=string:GUR_1202_CorAngar_SENDS_Info_08_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12823]
		blockName=string:GUR_1202_CorAngar_SENDS_Info_08_09
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12824]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12825]
				subType=enum:0
				text=string:Du hast dem Lager schon große Dienste erwiesen. Ich vertraue dir mehr als jedem anderen hier. 
				name=string:GUR_1202_CorAngar_SENDS_Info_08_09.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12826]
		blockName=string:GUR_1202_CorAngar_SENDS_Info_08_10
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12827]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12828]
				subType=enum:0
				text=string:Ich bitte dich, gehe zum Ork-Friedhof und berichte mir, was die Templer dort gefunden haben. 
				name=string:GUR_1202_CorAngar_SENDS_Info_08_10.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12829]
		blockName=string:GUR_1202_CorAngar_SENDS_EARN_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12830]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12831]
				subType=enum:0
				text=string:Was springt für mich dabei raus? 
				name=string:GUR_1202_CorAngar_SENDS_EARN_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 12832]
		blockName=string:GUR_1202_CorAngar_SENDS_EARN_Info_08_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 12833]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 12834]
				subType=enum:0
				text=string:Du willst Erz? Anscheinend habe ich mich in dir getäuscht. Aber ich brauche dich. Hier, 100 Erz. Mehr gibt es nicht! 
