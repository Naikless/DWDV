	[% zCCSBlock 0 26314]
		blockName=string:TPL_1402_GorNaToth_Teach_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26315]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26316]
				subType=enum:0
				text=string:Stärke und Geschicklichkeit sind ebenso wichtig wie geistige Kraft. 
