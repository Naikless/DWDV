		[]
	[]
	[% zCCSBlock 0 24271]
		blockName=string:SLD_709_Cord_TRAIN_14_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24272]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24273]
				subType=enum:0
				text=string:Gute Entscheidung! Um deine Technik zu verbessern, musst du erst einmal lernen, deine Waffe richtig zu halten. 
				name=string:SLD_709_Cord_TRAIN_14_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 24274]
		blockName=string:SLD_709_Cord_TRAIN_14_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24275]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24276]
				subType=enum:0
				text=string:Anfänger neigen dazu, Einhandwaffen mit beiden Händen zu halten. Mit so was fängst du am besten gar nicht erst an, das behindert dich nur. 
				name=string:SLD_709_Cord_TRAIN_14_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 24277]
		blockName=string:SLD_709_Cord_TRAIN_14_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24278]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24279]
				subType=enum:0
				text=string:Halt die Waffe mit einer Hand, Klinge nach oben, und dann immer schön schwingen lassen. 
				name=string:SLD_709_Cord_TRAIN_14_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 24280]
		blockName=string:SLD_709_Cord_TRAIN_14_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24281]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24282]
				subType=enum:0
				text=string:Du musst lernen, den Schwung deiner Waffe mit deinen Bewegungen in Einklang zu bringen, dann kannst du schneller zuschlagen. 
				name=string:SLD_709_Cord_TRAIN_14_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 24283]
		blockName=string:SLD_709_Cord_TRAIN_14_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24284]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24285]
				subType=enum:0
				text=string:Wenn du dich an meine Anweisungen hältst, wirst du in Zukunft eleganter und vor allem schneller kämpfen. 
				name=string:SLD_709_Cord_TRAIN_14_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 24286]
		blockName=string:SLD_709_Cord_TRAIN_14_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24287]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24288]
				subType=enum:0
				text=string:Ach, und noch was: Manche Treffer richten mehr Schaden an als andere! Als Anfänger hast du nur eine geringe Chance, solche kritischen Treffer zu landen. 
				name=string:SLD_709_Cord_TRAIN_14_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 24289]
		blockName=string:SLD_709_Cord_TRAIN_14_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24290]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24291]
				subType=enum:0
				text=string:Aber je weiter du fortschreitest, desto besser wird diese Fähigkeit. 
				name=string:SLD_709_Cord_TRAIN_14_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 24292]
		blockName=string:SLD_709_Cord_TRAINAGAIN_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24293]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24294]
				subType=enum:0
				text=string:Zeig mir, wie ich im Umgang mit Waffen noch besser werden kann. 
				name=string:SLD_709_Cord_TRAINAGAIN_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 24295]
		blockName=string:SLD_709_Cord_TRAINAGAIN_Info_14_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24296]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24297]
				subType=enum:0
				text=string:Na gut, die Grundkenntnisse hast du ja schon. Wenn du die Waffe gesenkt hältst, dann kannst du direkt mehr Wucht in deinen ersten Schlag legen. 
				name=string:SLD_709_Cord_TRAINAGAIN_Info_14_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 24298]
		blockName=string:SLD_709_Cord_TRAINAGAIN_Info_14_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24299]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24300]
				subType=enum:0
				text=string:Nutze den Schwung, weißt du noch? Gut, der nächste Schritt ist, dass du deinen Körper mehr einsetzt. Wenn du zweimal geschlagen hast, drehe dich um die eigene Achse, damit überrascht du den Gegner und bist in einer optimalen Position. 
				name=string:SLD_709_Cord_TRAINAGAIN_Info_14_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 24301]
		blockName=string:SLD_709_Cord_TRAINAGAIN_Info_14_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24302]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24303]
				subType=enum:0
				text=string:Dann ziehst du deine Klinge noch einmal quer von rechts nach links. 
				name=string:SLD_709_Cord_TRAINAGAIN_Info_14_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 24304]
		blockName=string:SLD_709_Cord_TRAINAGAIN_Info_14_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24305]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24306]
				subType=enum:0
				text=string:Und wieder von vorne. Vergiss nicht, Übung macht den Meister. Also, geh jetzt und werde ein wahrer Meister des einhändigen Kampfes. 
				name=string:SLD_709_Cord_TRAINAGAIN_Info_14_05.WAV
			[]
		[]
	[]
