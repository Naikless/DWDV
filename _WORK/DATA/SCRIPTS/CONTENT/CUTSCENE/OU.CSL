		subBlock0=float:0
		[% zCCSAtomicBlock 0 22295]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22296]
				subType=enum:0
				text=string:Corristo hat mich damals erwählt, weil ich eine besondere Sache für die Erzbarone erledigt habe. 
				name=string:DIA_Milten_WannaMage_02_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22297]
		blockName=string:DIA_Milten_WannaMage_02_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22298]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22299]
				subType=enum:0
