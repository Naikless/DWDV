		[]
	[]
	[% zCCSBlock 0 24706]
		blockName=string:DIA_SLD_753_Baloro_Worumgehts_ja_08_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 24707]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 24708]
				subType=enum:0
				text=string:Glaub mir, du wirst es nicht bereuen. Wie ich schon sagte: Wirklich jeden Gegner machst du damit platt! 
