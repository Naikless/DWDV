	[% zCCSBlock 0 27184]
		blockName=string:DIA_Guy_AdOC_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27185]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27186]
				subType=enum:0
				text=string:Was sind die Vorteile des Alten Lagers? 
				name=string:DIA_Guy_AdOC_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27187]
		blockName=string:DIA_Guy_AdOC_03_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27188]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27189]
				subType=enum:0
				text=string:Wenn du den Gardisten dein Schutzgeld zahlst, hast du ein ruhiges Leben. Sie mischen sich in alle Kämpfe ein, in denen jemand angegriffen wird, den sie beschützen. 
				name=string:DIA_Guy_AdOC_03_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27190]
		blockName=string:DIA_Guy_AdOC_03_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27191]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27192]
				subType=enum:0
				text=string:Außerdem kommt man nur hier günstig an Außenwelt-Waren. In den anderen Lagern sind diese Sachen unbezahlbar. 
				name=string:DIA_Guy_AdOC_03_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27193]
		blockName=string:DIA_Guy_AdOC_BACK_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27194]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27195]
				subType=enum:0
				text=string:Ich denke, ich hab' genug über's Alte Lager gehört... 
				name=string:DIA_Guy_AdOC_BACK_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27196]
		blockName=string:DIA_Guy_AdOC_BACK_03_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27197]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27198]
				subType=enum:0
				text=string:Also, mir gefällt's hier. 
				name=string:DIA_Guy_AdOC_BACK_03_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27199]
		blockName=string:DIA_Guy_AdOC_Warez_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27200]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27201]
				subType=enum:0
				text=string:Was sind hier wertvolle Waren? 
				name=string:DIA_Guy_AdOC_Warez_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27202]
		blockName=string:DIA_Guy_AdOC_Warez_03_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27203]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27204]
				subType=enum:0
				text=string:Alles, was aus der Außenwelt kommt. Brot, Käse, Schinken und vor allem Bier und Wein - nicht der billige Reisschnaps aus dem Neuen Lager, sondern richtig guter Stoff. 
				name=string:DIA_Guy_AdOC_Warez_03_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27205]
		blockName=string:DIA_Guy_AdOC_Warez_03_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27206]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27207]
				subType=enum:0
				text=string:Für Waffen bekommt man fast immer was und Rüstungen sind RICHTIG teuer. 
				name=string:DIA_Guy_AdOC_Warez_03_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27208]
		blockName=string:DIA_Guy_AdOC_Warez_03_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27209]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27210]
				subType=enum:0
				text=string:Die Gardisten haben sich die alten Rüstungen der Aufseher unter den Nagel gerissen, nachdem sie sie alle umgebracht hatten. 
				name=string:DIA_Guy_AdOC_Warez_03_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27211]
		blockName=string:DIA_Guy_AdOC_Warez_03_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27212]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27213]
				subType=enum:0
				text=string:Diese Rüstungen sind so was wie ihr Erkennungszeichen, die verkaufen sie nur an ihresgleichen, da kommst du als Fremder gar nicht ran. 
				name=string:DIA_Guy_AdOC_Warez_03_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27214]
		blockName=string:DIA_Guy_AdOC_Protection_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27215]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27216]
				subType=enum:0
				text=string:Und was ist, wenn zwei Leute kämpfen, die beide Schutzgeld zahlen? 
				name=string:DIA_Guy_AdOC_Protection_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27217]
		blockName=string:DIA_Guy_AdOC_Protection_03_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27218]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27219]
				subType=enum:0
				text=string:Dann sehen sich die Gardisten das in aller Seelenruhe an. Aber wehe, wenn der eine den anderen umbringt. Dann ist er selber dran! 
				name=string:DIA_Guy_AdOC_Protection_03_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27220]
		blockName=string:DIA_Guy_AdNC_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27221]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27222]
				subType=enum:0
				text=string:Was spricht für das Neue Lager? 
				name=string:DIA_Guy_AdNC_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27223]
		blockName=string:DIA_Guy_AdNC_03_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27224]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27225]
				subType=enum:0
				text=string:Wenn du kein Problem damit hast, wegen einem Stück Brot die Kehle durchgeschnitten zu kriegen, bist du da richtig! 
				name=string:DIA_Guy_AdNC_03_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27226]
		blockName=string:DIA_Guy_AdNC_03_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27227]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27228]
				subType=enum:0
				text=string:Nein - ganz so schlimm ist es nicht. Aber es geht anders zu als hier. 
				name=string:DIA_Guy_AdNC_03_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27229]
		blockName=string:DIA_Guy_AdNC_03_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27230]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27231]
				subType=enum:0
				text=string:Hier sorgt Gomez dafür, das alles halbwegs ruhig bleibt. Im Neuen Lager gibt es kein Gesetz. 
				name=string:DIA_Guy_AdNC_03_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27232]
		blockName=string:DIA_Guy_AdNC_03_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27233]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27234]
				subType=enum:0
				text=string:Die einzigen, die mächtig genug wären, den Laden zu schmeißen, sind die Wassermagier, und die forschen den ganzen Tag an der Barriere herum. 
				name=string:DIA_Guy_AdNC_03_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27235]
		blockName=string:DIA_Guy_AdST_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27236]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27237]
				subType=enum:0
				text=string:Warum sollte ich mich der Sekte anschließen? 
				name=string:DIA_Guy_AdST_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27238]
		blockName=string:DIA_Guy_AdST_03_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27239]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27240]
				subType=enum:0
				text=string:Tja, das fragst du am besten einen, der von dort kommt. 
				name=string:DIA_Guy_AdST_03_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27241]
		blockName=string:DIA_Guy_AdST_03_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27242]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27243]
				subType=enum:0
				text=string:Es sind immer einige Novizen im Lager, die können dir sicher 'ne Menge über die Sekte erzählen. 
				name=string:DIA_Guy_AdST_03_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27244]
		blockName=string:DIA_Guy_AdST_03_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27245]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27246]
				subType=enum:0
				text=string:Ich war selbst nie da. Aber nach allem, was man hört, sollen sie sehr großzügig sein. 
				name=string:DIA_Guy_AdST_03_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27247]
		blockName=string:DIA_Guy_AdST_03_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27248]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27249]
				subType=enum:0
				text=string:Keines der drei Lager scheint so dringend neue Leute zu brauchen wie das der Sekte. 
				name=string:DIA_Guy_AdST_03_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27250]
		blockName=string:DIA_Guy_MyOwnHut_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27251]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27252]
				subType=enum:0
				text=string:Welche von den Hütten kann ich für mich nehmen? 
				name=string:DIA_Guy_MyOwnHut_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27253]
		blockName=string:DIA_Guy_MyOwnHut_03_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27254]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27255]
				subType=enum:0
				text=string:Die Hütte da oben mit dem kleinen Vordach ist frei. 
				name=string:DIA_Guy_MyOwnHut_03_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27256]
		blockName=string:DIA_Guy_MyOwnHut_03_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27257]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27258]
				subType=enum:0
				text=string:Aber ich sag' dir vorher, du wirst die Hütte nur solange nehmen, wie du neu hier bist oder dem Alten Lager angehörst. 
				name=string:DIA_Guy_MyOwnHut_03_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27259]
		blockName=string:DIA_Guy_MyOwnHut_03_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27260]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27261]
				subType=enum:0
				text=string:Als Sektenspinner oder Bandit aus dem Neuen Lager, werden dich die Gardisten aus der Hütte prügeln. 
				name=string:DIA_Guy_MyOwnHut_03_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 27262]
		blockName=string:DIA_Guy_PrepareFight_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 27263]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 27264]
				subType=enum:0
				text=string:Beeil dich! Es geht gleich los! 
				name=string:DIA_Guy_PrepareFight_15_00.WAV
			[]
		[]
