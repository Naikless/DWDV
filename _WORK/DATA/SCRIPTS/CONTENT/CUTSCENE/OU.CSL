		blockName=string:DIA_Milten_WoTorrez_02_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22265]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22266]
				subType=enum:0
				text=string:Nachts ist er wie alle anderen drinnen - aber nur Magier dürfen das Tempelhaus betreten. Also kannst du ihn nur tagsüber sprechen. 
				name=string:DIA_Milten_WoTorrez_02_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22267]
		blockName=string:DIA_Milten_NochEinBrief_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22268]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22269]
				subType=enum:0
				text=string:Ich habe da noch einen Brief von den Wassermagiern. 
				name=string:DIA_Milten_NochEinBrief_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22270]
		blockName=string:DIA_Milten_NochEinBrief_02_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22271]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22272]
				subType=enum:0
				text=string:Ah! Sehr gut! Ich werde ihn an Corristo weiterleiten. 
				name=string:DIA_Milten_NochEinBrief_02_01.WAV
