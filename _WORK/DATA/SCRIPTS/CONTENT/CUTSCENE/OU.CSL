		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 16880]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 16881]
				subType=enum:0
				text=string:Cor Kalom hat mir viel beigebracht. Ich bin zwar noch nicht fertig mit meiner Ausbildung, aber ich kann dir auch etwas beibringen. 
				name=string:Nov_1301_Caine_LERNEN_Info_14_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 16882]
		blockName=string:Nov_1301_Caine_LERNEN_Info_14_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 16883]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 16884]
				subType=enum:0
				text=string:Ich verstehe mich sowohl auf Heiltränken, Manatränken und einigen speziellen Tränken, in dessen Geheimnisse nur die Gurus unserer Bruderschaft eingeweiht werden dürfen. 
				name=string:Nov_1301_Caine_LERNEN_Info_14_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 16885]
		blockName=string:Nov_1301_Caine_LERNEN_Info_14_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 16886]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 16887]
				subType=enum:0
				text=string:Die Rezepturen von Mana- und Heiltränken darf ich aber auch Novizen und Templern beibringen. 
				name=string:Nov_1301_Caine_LERNEN_Info_14_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 16888]
		blockName=string:Nov_1301_Caine_LERNEN_Info_14_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 16889]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 16890]
				subType=enum:0
				text=string:Ich kann dir nur was beibringen wenn du zu uns gehörst. 
				name=string:Nov_1301_Caine_LERNEN_Info_14_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 16891]
		blockName=string:Nov_1301_Caine_LERNEN_Info_14_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 16892]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 16893]
				subType=enum:0
				text=string:Cor Kalom würde mich umbringen, wenn ich die Rezepturen unserer Tränke Fremden beibringe. 
				name=string:Nov_1301_Caine_LERNEN_Info_14_06.WAV
			[]
		[]
