		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22277]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22278]
				subType=enum:0
				text=string:Erzähl mir was über die Feuermagier. 
				name=string:DIA_Milten_PERM_15_00.WAV
