		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17714]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17715]
				subType=enum:0
				text=string:Erlösung wovon? 
