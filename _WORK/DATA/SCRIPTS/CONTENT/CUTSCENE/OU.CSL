	[% zCCSBlock 0 14305]
		blockName=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14306]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14307]
				subType=enum:0
				text=string:Erkläre mir das Wesen der magischen Kreise. 
				name=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14308]
		blockName=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14309]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14310]
				subType=enum:0
				text=string:Natürlich. Die Kreise symbolisieren deinen Anteil an der magischen Macht.   
				name=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14311]
		blockName=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14312]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14313]
				subType=enum:0
				text=string:Sie stehen für Wissen, Begabung und für das Erlernen neuer Zaubersprüche.  
				name=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14314]
		blockName=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14315]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14316]
				subType=enum:0
				text=string:Du wirst jeden Kreis durchlaufen, bis du in den nächsten aufgenommen werden kannst. 
				name=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14317]
		blockName=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14318]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14319]
				subType=enum:0
				text=string:Es erfordert viele Lernstunden und es bedarf hoher Erfahrung, willst du die höheren Kreise erreichen.  
				name=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14320]
		blockName=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14321]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14322]
				subType=enum:0
				text=string:Doch die Mühe, sie zu erlernen, wird jedes Mal mit neuen machtvollen Zaubern belohnt. Die Kreise der Magie sind aber noch mehr als das. 
				name=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14323]
		blockName=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14324]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14325]
				subType=enum:0
				text=string:Sie sind ein Teil deines Lebens. Sie werden dich immer begleiten. Mache sie zu einem Teil von dir. 
				name=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14326]
		blockName=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14327]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14328]
				subType=enum:0
				text=string:Ihre Macht zu verstehen, bedeutet deine eigene Macht zu erkennen. 
				name=string:KDF_402_Corristo_EXPLAINCIRCLES_Info_14_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14329]
		blockName=string:KDF_402_Corristo_KREIS1_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14330]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14331]
				subType=enum:0
				text=string:Ich bin jetzt bereit, den ersten Kreis zu erlernen. 
				name=string:KDF_402_Corristo_KREIS1_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14332]
		blockName=string:KDF_402_Corristo_KREIS1_Info_14_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14333]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14334]
				subType=enum:0
				text=string:Den ersten der magischen Kreise zu betreten, bedeutet die Anwendung magischer Runen erlernen. 
				name=string:KDF_402_Corristo_KREIS1_Info_14_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14335]
		blockName=string:KDF_402_Corristo_KREIS1_Info_14_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14336]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14337]
				subType=enum:0
				text=string:Jede Rune birgt in sich die Struktur eines speziellen Zaubers. 
				name=string:KDF_402_Corristo_KREIS1_Info_14_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14338]
		blockName=string:KDF_402_Corristo_KREIS1_Info_14_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14339]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14340]
				subType=enum:0
				text=string:Durch den Einsatz deiner eigenen magischen Energie, bist du in der Lage, den Zauber der Rune zu wirken. 
				name=string:KDF_402_Corristo_KREIS1_Info_14_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14341]
		blockName=string:KDF_402_Corristo_KREIS1_Info_14_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14342]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14343]
				subType=enum:0
				text=string:Doch anders als Spruchrollen, die auch eine Zauberformel sind, sorgt die magische Kraft, die der Rune innewohnt dafür, dass die Struktur des Zaubers erhalten bleibt.  
				name=string:KDF_402_Corristo_KREIS1_Info_14_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14344]
		blockName=string:KDF_402_Corristo_KREIS1_Info_14_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14345]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14346]
				subType=enum:0
				text=string:Jede Rune ist eine Quelle magischer Kraft, auf die du immer wieder zurückgreifen kannst.   
				name=string:KDF_402_Corristo_KREIS1_Info_14_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14347]
		blockName=string:KDF_402_Corristo_KREIS1_Info_14_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14348]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14349]
				subType=enum:0
				text=string:Deine eigene magische Kraft verbraucht sich beim Benutzen der Rune, wie bei einer Spruchrolle. 
				name=string:KDF_402_Corristo_KREIS1_Info_14_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14350]
		blockName=string:KDF_402_Corristo_KREIS1_Info_14_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14351]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14352]
				subType=enum:0
				text=string:Mit jedem Kreis, den du lernst, lernst du auch die Möglichkeit weitere Runen zu benutzen. 
				name=string:KDF_402_Corristo_KREIS1_Info_14_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14353]
		blockName=string:KDF_402_Corristo_KREIS1_Info_14_09
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14354]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14355]
				subType=enum:0
				text=string:Nutze die Kraft der Runen, um dich selber zu erkennen. 
				name=string:KDF_402_Corristo_KREIS1_Info_14_09.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14356]
		blockName=string:KDF_402_Corristo_KREIS2_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14357]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14358]
				subType=enum:0
				text=string:Ich möchte den zweiten Kreis erlernen. 
				name=string:KDF_402_Corristo_KREIS2_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14359]
		blockName=string:KDF_402_Corristo_KREIS2_Info_14_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14360]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14361]
				subType=enum:0
				text=string:Du hast das Verständnis der Runen gelernt. Nun ist es an der Zeit, dieses Verständnis zu vertiefen. 
				name=string:KDF_402_Corristo_KREIS2_Info_14_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14362]
		blockName=string:KDF_402_Corristo_KREIS2_Info_14_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14363]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14364]
				subType=enum:0
				text=string:Mit dem zweiten Kreis lernst du die Grundsätze stärkerer Kampfzauber und vor allem das Geheimnis der Heilung.  
				name=string:KDF_402_Corristo_KREIS2_Info_14_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14365]
		blockName=string:KDF_402_Corristo_KREIS2_Info_14_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14366]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14367]
				subType=enum:0
				text=string:Doch du musst noch viel lernen, willst du die tatsächlichen Geheimnisse der Magie ergründen.  
				name=string:KDF_402_Corristo_KREIS2_Info_14_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14368]
		blockName=string:KDF_402_Corristo_KREIS2_Info_14_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14369]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14370]
				subType=enum:0
				text=string:Du weißt, dass du jede Rune solange einsetzen kannst, bis deine eigene Kraft verbraucht ist. 
				name=string:KDF_402_Corristo_KREIS2_Info_14_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14371]
		blockName=string:KDF_402_Corristo_KREIS2_Info_14_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14372]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14373]
				subType=enum:0
				text=string:Aber frage dich, ob es sinnvoll ist. Du hast Macht verliehen bekommen, mit der es dir ein Leichtes ist, Tod und Verderben zu bringen.    
				name=string:KDF_402_Corristo_KREIS2_Info_14_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14374]
		blockName=string:KDF_402_Corristo_KREIS2_Info_14_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14375]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14376]
				subType=enum:0
				text=string:Doch der wahrhafte Magier zaubert nur dann, wenn es angebracht ist. 
				name=string:KDF_402_Corristo_KREIS2_Info_14_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14377]
		blockName=string:KDF_402_Corristo_KREIS2_Info_14_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14378]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14379]
				subType=enum:0
				text=string:Erkenne die Situation, dann wirst du die Macht der Runen erkennen.    
				name=string:KDF_402_Corristo_KREIS2_Info_14_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14380]
		blockName=string:KDF_402_Corristo_KREIS3_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14381]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14382]
				subType=enum:0
				text=string:Kannst du mich den dritten Kreis lehren? 
				name=string:KDF_402_Corristo_KREIS3_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14383]
		blockName=string:KDF_402_Corristo_KREIS3_Info_14_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14384]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14385]
				subType=enum:0
				text=string:Der dritte Kreis der Magie ist einer der wichtigsten Schritte im Leben eines Magiers. Nicht länger bist du ein Suchender.  
				name=string:KDF_402_Corristo_KREIS3_Info_14_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14386]
		blockName=string:KDF_402_Corristo_KREIS3_Info_14_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14387]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14388]
				subType=enum:0
				text=string:Du bist bereits ein Stück des magischen Weges gegangen. Du hast gelernt, die Runen zu nutzen.  
				name=string:KDF_402_Corristo_KREIS3_Info_14_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14389]
		blockName=string:KDF_402_Corristo_KREIS3_Info_14_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14390]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14391]
				subType=enum:0
				text=string:Dieses Wissen ist das Fundament für deinen weiteren Weg. Nutze die Runen mit Bedacht.  
				name=string:KDF_402_Corristo_KREIS3_Info_14_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14392]
		blockName=string:KDF_402_Corristo_KREIS3_Info_14_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14393]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14394]
				subType=enum:0
				text=string:Du kannst eine Rune nutzen oder nicht. Aber entscheide dich für eine der Möglichkeiten. 
				name=string:KDF_402_Corristo_KREIS3_Info_14_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14395]
		blockName=string:KDF_402_Corristo_KREIS3_Info_14_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14396]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14397]
				subType=enum:0
				text=string:Zögere nicht, deine Macht einzusetzen, wenn du dich einmal entscheiden hast. 
				name=string:KDF_402_Corristo_KREIS3_Info_14_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14398]
		blockName=string:KDF_402_Corristo_KREIS3_Info_14_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14399]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14400]
				subType=enum:0
				text=string:Erkenne deinen Weg, dann wirst du die Macht der Entscheidung erkennen. 
				name=string:KDF_402_Corristo_KREIS3_Info_14_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14401]
		blockName=string:KDF_402_Corristo_KREIS4_Info_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14402]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14403]
				subType=enum:0
				text=string:Ich bin bereit für den vierten der Kreise. 
				name=string:KDF_402_Corristo_KREIS4_Info_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14404]
		blockName=string:KDF_402_Corristo_KREIS4_Info_14_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14405]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14406]
				subType=enum:0
				text=string:Du hast die ersten drei Kreise durchschritten. Nun ist es an der Zeit, dass du die Geheimnisse der Magie kennenlernst. 
				name=string:KDF_402_Corristo_KREIS4_Info_14_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14407]
		blockName=string:KDF_402_Corristo_KREIS4_Info_14_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14408]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14409]
				subType=enum:0
				text=string:Die Magie der Runen ruht in ihrem Stein. Es sind magische Steine, gewonnen aus magischem Erz.  
				name=string:KDF_402_Corristo_KREIS4_Info_14_03.WAV
			[]
		[]
