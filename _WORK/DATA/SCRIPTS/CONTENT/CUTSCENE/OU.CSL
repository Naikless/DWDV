			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 23214]
				subType=enum:0
				text=string:Die Magier belohnen ihre Boten immer sehr gut, und die meisten hier haben gar nichts, verstehst du? 
				name=string:Info_Diego_Brief_11_04.WAV
