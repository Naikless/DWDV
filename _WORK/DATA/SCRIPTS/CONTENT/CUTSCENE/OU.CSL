	[% zCCSBlock 0 17779]
		blockName=string:DIA_BaalTaran_Bruderschaft_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17780]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17781]
				subType=enum:0
				text=string:Wir sind eine Gemeinschaft von Anhängern eines neuen Glaubens. 
				name=string:DIA_BaalTaran_Bruderschaft_05_01.WAV
