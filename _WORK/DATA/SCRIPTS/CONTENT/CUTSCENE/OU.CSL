		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14636]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14637]
				subType=enum:0
				text=string:Die Orks sind nicht etwa, wie viele glauben, Tiere. Ihre Kultur reicht fast ebenso weit zurück, wie die der Menschen. 
				name=string:Info_Xardas_BARRIER_14_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14638]
		blockName=string:Info_Xardas_BARRIER_14_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14639]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14640]
				subType=enum:0
				text=string:Vor vielen hundert Jahren beschwörten fünf Ork-Schamanen einen uralten Erzdämonen, um ihrem Klan die Macht zu verleihen, ihre Feinde zu vernichten. 
				name=string:Info_Xardas_BARRIER_14_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 14641]
		blockName=string:Info_Xardas_BARRIER_15_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 14642]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 14643]
