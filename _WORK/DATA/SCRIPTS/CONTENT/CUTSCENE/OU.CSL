		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17708]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17709]
				subType=enum:0
				text=string:Seit er vor fünf Jahren das erste Mal mit Y'Berion in Kontakt trat, hat er uns geleitet. 
