		[]
	[]
	[% zCCSBlock 0 23230]
		blockName=string:Info_Diego_WarumGeholfen_11_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 23231]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 23232]
				subType=enum:0
				text=string:Und das konnte ich nicht mit ansehen. Schließlich bin ich den ganzen weiten Weg gekommen um dir einen Vorschlag zu machen. 
				name=string:Info_Diego_WarumGeholfen_11_02.WAV
			[]
		[]
	[]
