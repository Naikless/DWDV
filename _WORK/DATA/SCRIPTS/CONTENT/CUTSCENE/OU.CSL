		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17696]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17697]
				subType=enum:0
				text=string:Du siehst, in unserer Bruderschaft gibt es viele Möglichkeiten für dich. 
				name=string:DIA_BaalParvez_MyAdvantage_10_04.WAV
			[]
		[]
	[]
