		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 15770]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 15771]
				subType=enum:0
				text=string:Der Sturm des Feuers, die Welle aus Eis und die Macht, Untote zu vernichten, sind wahrhaft mächtige Sprüche, deren Runen dir nun untertan sind. 
				name=string:KDW_600_Saturas_KREIS5_Info_14_03.WAV
