		[]
	[]
	[% zCCSBlock 0 22279]
		blockName=string:DIA_Milten_PERM_02_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22280]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22281]
				subType=enum:0
				text=string:Der Kreis des Feuers bezieht seine Macht von der Gnade Innos. Der Gott des Lichts, der Wahrheit und der Gerechtigkeit wacht über uns. 
				name=string:DIA_Milten_PERM_02_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22282]
		blockName=string:DIA_Milten_PERM_02_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22283]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22284]
				subType=enum:0
				text=string:Die Magier betrachten sich selber als die Erwählten - und so behandeln sie auch alle anderen. 
				name=string:DIA_Milten_PERM_02_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22285]
		blockName=string:DIA_Milten_PERM_02_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22286]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22287]
				subType=enum:0
				text=string:Ich hatte das Glück, damals aufgenommen zu werden. Corristo hat mich zu seinem Lehrling gemacht. Vermutlich hatte er einfach Lust auf Unterhaltung. 
				name=string:DIA_Milten_PERM_02_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22288]
		blockName=string:DIA_Milten_PERM_02_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22289]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22290]
				subType=enum:0
				text=string:Seitdem bemühe ich mich jeden Tag, meine magischen Fähigkeiten zu steigern, damit ich in den nächsten Kreis aufgenommen werden kann. 
				name=string:DIA_Milten_PERM_02_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22291]
		blockName=string:DIA_Milten_WannaMage_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22292]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22293]
				subType=enum:0
				text=string:Ich will ein Lehrling der Magier werden - so wie du. 
				name=string:DIA_Milten_WannaMage_15_00.WAV
			[]
