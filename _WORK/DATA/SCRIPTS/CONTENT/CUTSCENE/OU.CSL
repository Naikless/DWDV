		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22820]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22821]
				subType=enum:0
				text=string:Und daran glaubst du? 
				name=string:DIA_Lester_CampInfo_SLEEPER_15_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22822]
		blockName=string:DIA_Lester_CampInfo_SLEEPER_05_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22823]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22824]
				subType=enum:0
				text=string:Hey, ich bin seit zwei Jahren in der Barriere. Weißt du überhaupt wie lange ZWEI JAHRE sein können? 
				name=string:DIA_Lester_CampInfo_SLEEPER_05_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22825]
		blockName=string:DIA_Lester_CampInfo_SLEEPER_05_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22826]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22827]
				subType=enum:0
				text=string:Du kannst dir nicht vorstellen, was ich alles bereit bin zu glauben und zu machen, damit ich hier wieder raus komme! 
