		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 16847]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 16848]
				subType=enum:0
				text=string:Crawler findest du in der Alten Mine. Aber du musst wissen wie du ihnen das Sekret abzapfst. 
				name=string:DIA_Caine_WoSekret_13_01.WAV
