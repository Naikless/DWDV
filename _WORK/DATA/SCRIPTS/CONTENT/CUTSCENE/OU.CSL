		subBlock0=float:0
		[% zCCSAtomicBlock 0 26294]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26295]
				subType=enum:0
				text=string:Ich möchte eine schwere Templer-Rüstung haben. 
				name=string:Info_GorNaToth_ARMOR_H_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26296]
		blockName=string:Info_GorNaToth_ARMOR_H_11_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26297]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26298]
				subType=enum:0
				text=string:Du bist noch nicht so weit! Bewähre dich erst noch weiter für unsere Gemeinschaft, dann wirst dir auch das Recht zuteil, solch eine erlesene Rüstung zu tragen. 
				name=string:Info_GorNaToth_ARMOR_H_11_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26299]
		blockName=string:Info_GorNaToth_ARMOR_H_11_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26300]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26301]
				subType=enum:0
				text=string:Du hast dir mittlerweile das Recht verdient, einen solchen Panzer zu tragen, doch du bist zurzeit nicht in der Lage, eine ausreichende Spende an die Gemeinschaft zu leisten! 
				name=string:Info_GorNaToth_ARMOR_H_11_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26302]
		blockName=string:Info_GorNaToth_ARMOR_H_11_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26303]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26304]
				subType=enum:0
				text=string:Trage diese Rüstung fortan als Zeichen der höchsten Ehre! 
				name=string:Info_GorNaToth_ARMOR_H_11_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 26305]
		blockName=string:Info_GorNaToth_ARMOR_BACK_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 26306]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 26307]
				subType=enum:0
				text=string:Ich hab' es mir anders überlegt! 
				name=string:Info_GorNaToth_ARMOR_BACK_15_01.WAV
