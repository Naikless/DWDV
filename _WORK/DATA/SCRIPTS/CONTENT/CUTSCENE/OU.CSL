		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17720]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17721]
				subType=enum:0
				text=string:Was hat euch der Schläfer gesagt? 
				name=string:DIA_BaalParvez_SleeperSaid_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17722]
		blockName=string:DIA_BaalParvez_SleeperSaid_10_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17723]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17724]
				subType=enum:0
				text=string:Er hat uns in den Sumpf geführt, zu den alten Tempelruinen. Dort haben wir unser Lager errichtet. 
				name=string:DIA_BaalParvez_SleeperSaid_10_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17725]
		blockName=string:DIA_BaalParvez_SleeperSaid_10_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17726]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17727]
				subType=enum:0
				text=string:Er hat uns die Unabhängigkeit gegeben - keiner von uns muss mehr in den Minen schuften. 
				name=string:DIA_BaalParvez_SleeperSaid_10_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17728]
		blockName=string:DIA_BaalParvez_SleeperSaid_10_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17729]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17730]
				subType=enum:0
				text=string:Er hat uns die Wirkung des Sumpfkrauts gezeigt. Wir verkaufen es an die anderen Lager und bekommen dafür alles, was wir brauchen. 
				name=string:DIA_BaalParvez_SleeperSaid_10_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 17731]
		blockName=string:DIA_BaalParvez_SleeperSaid_10_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17732]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17733]
				subType=enum:0
				text=string:Und er gab einigen von uns die Gabe der Magie. Einer alten, mächtigen Magie, verschieden von der, die die Magier des Reiches anwenden. 
				name=string:DIA_BaalParvez_SleeperSaid_10_04.WAV
			[]
		[]
	[]
