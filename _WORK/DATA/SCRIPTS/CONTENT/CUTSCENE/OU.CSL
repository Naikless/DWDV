		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22814]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22815]
				subType=enum:0
				text=string:Der Schläfer ist ein göttliches Wesen. Er schenkt uns Visionen - zumindest den Gurus. 
				name=string:DIA_Lester_CampInfo_SLEEPER_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 22816]
		blockName=string:DIA_Lester_CampInfo_SLEEPER_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 22817]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 22818]
				subType=enum:0
				text=string:Wir beten zu ihm, denn er wird uns allen die Freiheit schenken. 
				name=string:DIA_Lester_CampInfo_SLEEPER_05_02.WAV
			[]
		[]
	[]
