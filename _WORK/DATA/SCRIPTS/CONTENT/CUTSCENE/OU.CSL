		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 17693]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 17694]
				subType=enum:0
				text=string:Wenn der Schläfer mit dir ist, kannst du sogar beides kombinieren - die besten unter den Templern haben nämlich auch die Gabe der Magie. 
				name=string:DIA_BaalParvez_MyAdvantage_10_03.WAV
			[]
		[]
