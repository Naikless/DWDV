		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 13517]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 13518]
				subType=enum:0
				text=string:Der Schläfer führte Y'Berion und seine getreuen Anhänger an diesen Ort. Hier ließen sie sich nieder. 
				name=string:Gur_1208_BaalCadar_KREIS2_Info_02_03.WAV
